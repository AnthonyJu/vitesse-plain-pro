{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.12.1",
  "scripts": {
    "dev": "vite",
    "lint": "eslint --fix \"src/**/*.{js,jsx,ts,tsx,vue}\"",
    "lint:style": "stylelint --fix \"src/**/*.{vue,scss}\"",
    "typecheck": "vue-tsc --noEmit",
    "build": "vite build",
    "build-zip": "vite build && pick-zip",
    "netlify": "export NODE_OPTIONS=--max_old_space_size=4096 && vite build",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "up": "taze major -I",
    "sizecheck": "npx vite-bundle-visualizer",
    "postinstall": "npx simple-git-hooks"
  },
  "dependencies": {
    "@arcgis/core": "^4.30.9",
    "@vueuse/core": "^11.1.0",
    "@vueuse/integrations": "^11.1.0",
    "axios": "^1.7.7",
    "cropperjs": "^1.6.2",
    "echarts": "^5.5.1",
    "echarts-liquidfill": "^3.1.0",
    "echarts-wordcloud": "^2.1.0",
    "element-plus": "^2.8.4",
    "file-saver": "^2.0.5",
    "howler": "^2.2.4",
    "konva": "^9.3.15",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.2.4",
    "pinia-plugin-persistedstate": "^4.1.1",
    "print-js": "^1.6.0",
    "qrcode": "^1.5.4",
    "shikiji": "^0.10.2",
    "vue": "^3.5.11",
    "vue-countup-v3": "^1.4.2",
    "vue-draggable-resizable": "3.0.0-beta.2",
    "vue-echarts": "^7.0.3",
    "vue-grid-layout": "3.0.0-beta1",
    "vue-i18n": "^10.0.4",
    "vue-request": "^2.0.4",
    "vue-router": "^4.4.5",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.3",
    "@anthony-ju/eslint-config": "^0.2.8",
    "@anthony-ju/stylelint-config": "^0.3.0",
    "@iconify/vue": "^4.1.2",
    "@intlify/unplugin-vue-i18n": "^5.2.0",
    "@types/file-saver": "^2.0.7",
    "@types/howler": "^2.2.12",
    "@types/node": "^22.7.5",
    "@types/nprogress": "^0.2.3",
    "@unocss/eslint-config": "^0.63.4",
    "@vitejs/plugin-vue": "^5.1.4",
    "critters": "^0.0.24",
    "cross-env": "^7.0.3",
    "eslint": "^9.12.0",
    "eslint-plugin-format": "^0.1.2",
    "https-localhost": "^4.7.1",
    "javascript-obfuscator": "^4.1.1",
    "lint-staged": "^15.2.10",
    "pick-zip": "^0.1.0",
    "picocolors": "^1.1.0",
    "pnpm": "^9.12.1",
    "rollup-plugin-obfuscator": "^1.1.0",
    "sass": "^1.77.6",
    "simple-git-hooks": "^2.11.1",
    "stylelint": "^16.9.0",
    "taze": "^0.17.2",
    "typescript": "^5.6.3",
    "unocss": "^0.63.4",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-element-plus": "^0.8.0",
    "unplugin-vue-components": "^0.27.4",
    "unplugin-vue-router": "^0.10.8",
    "vite": "^5.4.8",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-pwa": "^0.20.5",
    "vite-plugin-vue-devtools": "^7.4.6",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^2.1.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "node scripts/verify-commit.js"
  },
  "lint-staged": {
    "*.{vue,scss}": "stylelint --fix",
    "*.{js,jsx,ts,tsx,vue}": "eslint --fix"
  }
}
